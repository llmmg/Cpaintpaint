t=40;
for(i=0, i<400, i=(i+t)){
    for(j=0, j<400, j=(j+t)){
      if((i%(t*2))<t){
        if((j%(t*2))<t){
          drawRectangle(i,j,i+t,j+t,255,255,255)
        }
      };
      if((i%(t*2))>=t){
        if((j%(t*2))>=t){
          drawRectangle(i,j,i+t,j+t,255,255,255)
        }
      }
    }
}
